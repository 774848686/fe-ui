{
  "name": "one-piece-ui",
  "version": "0.0.49",
  "private": false,
  "main": "lib/feui.common.js",
  "files": [
    "lib",
    "packages"
  ],
  "scripts": {
    "init": "npm install commitizen -g && commitizen init cz-conventional-changelog --save-dev --save-exact && npm run bootstrap",
    "bootstrap": "npm install && cd ./packages/theme-chalk && npm install",
    "build:style": "gulp build --gulpfile packages/theme-chalk/gulpfile.js && cp-cli packages/theme-chalk/lib lib/theme-chalk",
    "build:docs": "vue-cli-service build",
    "build:lib": "node build/build-lib.js",
    "build:entry": "node build/build-entry.js ",
    "serve": "vue-cli-service serve",
    "clean": "rimraf lib && rimraf packages/*/lib",
    "deploy": "sh build/deploy.sh",
    "lint": "vue-cli-service lint",
    "lib": "vue-cli-service build --target lib --name feui --dest lib packages/index.js && webpack --config ./build/webpack.component.js"
  },
  "dependencies": {
    "core-js": "^3.4.4",
    "webpack-cli": "^3.3.11"
  },
  "peerDependencies": {
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "@vue/component-compiler-utils": "^3.1.2",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-component": "^1.1.1",
    "cp-cli": "^2.0.0",
    "cz-conventional-changelog": "^3.2.0",
    "element-ui": "^2.13.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "highlight.js": "^9.3.0",
    "markdown-it": "^12.3.2",
    "markdown-it-anchor": "^5.2.7",
    "markdown-it-chain": "^1.3.0",
    "markdown-it-container": "^2.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "progress-bar-webpack-plugin": "^2.1.0",
    "sass-loader": "^8.0.0",
    "shelljs": "^0.8.4",
    "signale": "^1.4.0",
    "transliteration": "^2.1.8",
    "uppercamelcase": "^3.0.0",
    "vue": "^2.6.11",
    "vue-markdown-loader": "^2.4.1",
    "vue-router": "^3.3.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {
      "off": 0,
      "warn": 1,
      "no-trailing-spaces": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:774848686/fe-ui.git"
  },
  "keywords": [
    "vue",
    "fe-ui",
    "one piece"
  ],
  "description": "learning for element",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
